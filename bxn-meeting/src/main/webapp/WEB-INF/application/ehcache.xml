<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
    <diskStore path="java.io.tmpdir"/>
    <defaultCache
            eternal="false"
            maxElementsInMemory="1000"
            overflowToDisk="true"
            diskPersistent="true"
            timeToLiveSeconds="300"
            />
    <cache name="RestUserCenterServiceInvokerResult"
               eternal="false"
               maxElementsInMemory="100"
               overflowToDisk="false"
               diskPersistent="false"
               timeToIdleSeconds="0"
               timeToLiveSeconds="300"
               memoryStoreEvictionPolicy="LRU" />
   <cache name="RestTeachCenterServiceInvokerResult"
               eternal="false"
               maxElementsInMemory="100"
               overflowToDisk="false"
               diskPersistent="false"
               timeToIdleSeconds="0"
               timeToLiveSeconds="300"
               memoryStoreEvictionPolicy="LRU" /> 
   <cache name="RestServiceLocation"
	          eternal="false"
	          maxElementsInMemory="100"
	          overflowToDisk="false"
	          diskPersistent="false"
	          timeToIdleSeconds="0"
	          timeToLiveSeconds="300"
	          memoryStoreEvictionPolicy="LRU" />
    <cache name="TeachClientCache"
 	 		  eternal="false"
			  maxElementsInMemory="300"
			  overflowToDisk="false"
			  diskPersistent="false"
			  timeToIdleSeconds="0"
			  timeToLiveSeconds="300"
			  memoryStoreEvictionPolicy="LRU" />
   <cache name="sys_sequence"
               eternal="false"
               maxElementsInMemory="100"
               overflowToDisk="false"
               diskPersistent="false"
               timeToIdleSeconds="0"
               timeToLiveSeconds="300"
               memoryStoreEvictionPolicy="LRU" /> 
   <cache name="sys_bizsystem"
               eternal="false"
               maxElementsInMemory="100"
               overflowToDisk="false"
               diskPersistent="false"
               timeToIdleSeconds="0"
               timeToLiveSeconds="30000"
               memoryStoreEvictionPolicy="LRU" />
   <cache name="RestServiceAppkeyAuthentication"
           eternal="false"
           maxElementsInMemory="100"
           overflowToDisk="false"
           diskPersistent="false"
           timeToIdleSeconds="0"
           timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LRU" />
</ehcache>